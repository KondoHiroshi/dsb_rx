  <node pkg = "dsb_rx"
        name = "sis_bias_vol_locker"
        type = "relay.py">
    <param name = "config_file"
           value = "$(find dsb_rx)/relay_conf/sis_bias_vol_locker.conf" />
  </node>
  -->
